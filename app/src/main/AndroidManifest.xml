<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.deedeveloper.realmext">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".categories.SixthActivity" />

        <activity android:name=".categories.FifthActivity" />
        <activity android:name=".categories.ForthActivity" />
        <activity android:name=".categories.CromActivity" />
        <activity android:name=".categories.GcamActivity" />

        <activity
            android:name=".categories.StockRom"
            android:theme="@style/AppTheme.NoActionBar" />

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>